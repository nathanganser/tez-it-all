.view-navbar {
   @apply w-full px-6 py-2 z-2
   flex items-center
   bg-default-elevated border border-transparent border-b-black/10
   sticky top-0 left-0;

   &-brand {
      @apply flex items-center;

      &-logo {
         @apply w-42px mr-4 md:w-48px;
      }

      &-label {
         @apply text-xl sm:text-2xl font-medium;
      }
   }
}
